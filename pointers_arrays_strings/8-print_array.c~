#include "main.h"
/**
*print_array - print n element of array
*@a: array
*@n: number of elements
*Return: nothing
*/
void print_array(int *a, int n)
{
int inc;
for (compt_bcle = 0; compt_bcle < n ; inc++)
{
if (compt_bcle != n - 1)
printf("%d, ", a[compt_bcle]);
else
printf("%d", a[compt_bcle]);

}
printf("\n");
}
