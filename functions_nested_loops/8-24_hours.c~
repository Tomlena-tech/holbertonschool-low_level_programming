#include "main.h"
#include <unistd.h>
/**
* jack_bauer - Prints every minute of the day of Jack Bauer
*
* Return: Nothing
*/
void jack_bauer(void)
{
int hours
int minutes

if(hours = 0; hours <24; hours++)
{	   
if(minutes = 0; mintes < 60; minutes++)
{
_putchar('0' + (hour / 10));
_putchar('0' + (hour % 10));
_putchar(':');
_putchar('0' + (minute / 10));
_putchar('0' + (minute % 10));
_putchar ('\n');
 }
 }
}
