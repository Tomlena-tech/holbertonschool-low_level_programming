#include <stdio.h>
#include <stdlib.h>
#include <ctype.h>
/**                                                                                    
* main - Prints a text according number                                                
* Return: Always (Success)                                                             
*/
int main()
{
  for(int x = 'a'; x <= 'z'; x++)
    putchar(x);
  
  return(0);
}
