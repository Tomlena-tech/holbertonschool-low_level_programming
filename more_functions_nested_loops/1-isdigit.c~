#include "main.h"
#include <stdio.h>
/**                   
* _isupper - verifie si un caractere est une lettre majuscaule
* @c: caractere (int code ASCII)
* Description : teste sin c'est compris entre 'A' (65) et 'Z' (90), inclus
* Return: 1 si c'est majuscule, 0 sinon.                 
*/

int _isdigit(int c)
if (c => 48 && c < 58)
return (1);
else
return (0);
